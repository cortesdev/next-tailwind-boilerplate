{
  "name": "cortesdev-next-boilerplate",
  "description": "Cortesdev Boilerplate Template ts next tailwind",
  "version": "0.5.0",
  "private": "true",
  "packageManager": "pnpm@7.9.5",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext=jsx,ts,tsx .",
    "lint:fix": "eslint --ext=jsx,ts,tsx . --fix",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.7.0",
    "next": "^13.2.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.35.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.tsx,*.ts,*.mdx": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": {
    "email": "giralander@gmail.com",
    "name": "Ricardo Cortes",
    "url": "https://cortesdev.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cortesdev/cortesdev-next-boilerplate/issues"
  },
  "homepage": "https://cortesdev.de",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "tailwindcss",
    "template",
    "radix",
    "react",
    "nextjs"
  ]
}
